{
    "list.source.grep.command": "rg",
    "list.source.grep.args": [
        "--smart-case"
    ],
    "suggest.minTriggerInputLength": 3,
    "suggest.autoTrigger":"always",
    "suggest.triggerAfterInsertEnter": true,
    "suggest.enablePreview": true,
    "suggest.floatEnable": true,
    "suggest.echodocSupport": false,
    "codeLens.enable": true,
    "diagnostic.enable": false,
    "suggest.noselect": false,
    "coc.preferences.formatOnSaveFiletypes": [
        "rust",
        "go"
    ],
    "coc.preferences.hoverTarget": "float",
    "python.jediEnabled": true,
    "jedi.enable": true,
    "jedi.startupMessage": true,
    "jedi.markupKindPreferred": "markdown",
    "languageServer": { },
    "rust-analyzer.serverPath": "/usr/local/bin/rust-analyzer"
}
